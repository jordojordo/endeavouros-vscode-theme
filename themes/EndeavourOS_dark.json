  "tokenColors": [
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other",
        "keyword.function"
      ],
      "settings": {
        "foreground": "#7f7fff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage & Types",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier",
        "storage.type.ts",
        "storage.type.js"
      ],
      "settings": {
        "foreground": "#cdccdb"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e3e3ea"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "comment.block.js",
        "meta.class.js",
        "meta.export.default.js",
        "source.js",
        "punctuation.definition.comment.ts",
        "comment.block.documentation.ts",
        "source.ts"
      ],
      "settings": {
        "foreground": "#b1b1ba"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#e3e3ea"
      }
    },
    {
      "name": "Entities",
      "scope": "entity",
      "settings": {
        "foreground": "#7f5dbf"
      }
    },
    {
      "name": "Function Declarations",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#cc3980"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#ff7f7f"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#27ae60"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#7fbaff"
      }
    },
    {
      "name": "Markup Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#7f5dbf"
      }
    },
    {
      "name": "Markup Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ff7f7f"
      }
    },
    {
      "name": "Markup Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#27ae60"
      }
    },
    {
      "name": "Markup Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#7f3fbf"
      }
    },
    {
      "name": "Markup Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#7f7fff"
      }
    },
    {
      "name": "Markup List & Raw",
      "scope": ["markup.list", "markup.raw"],
      "settings": {
        "foreground": "#e3e3ea"
      }
    }
  ],
  "semanticTokenColors": {
    "newOperator": "#7fbaff",
    "stringLiteral": "#27ae60",
    "customLiteral": "#cdccdb",
    "numberLiteral": "#b5cea8"
  }
